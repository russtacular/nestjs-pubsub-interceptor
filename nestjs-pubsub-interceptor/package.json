{
  "name": "@hiphops/nestjs-pubsub-interceptor",
  "version": "0.4.2",
  "description": "A NestJS Interceptor to decode and unwrap GCP Pub/Sub push messages",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hiphops-io/nestjs-pubsub-interceptor"
  },
  "bugs": "https://github.com/hiphops-io/nestjs-pubsub-interceptor",
  "author": "manterfield <tom@hiphops.io>",
  "license": "MIT",
  "private": false,
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs",
    "gcp",
    "pubsub",
    "interceptor"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@nestjs/common": ">9.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/common": "^11.1.5",
    "@nestjs/core": "^11.1.5",
    "@types/express": "^5.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "^24.0.14",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.7",
    "eslint-plugin-prettier": "^4.2.3",
    "jest": "30.0.4",
    "prettier": "^3.6.2",
    "rxjs": "^7.8.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.3",
    "ts-jest": "29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
